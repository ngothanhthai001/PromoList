<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="customer_account_navigation" >
            <block class="Magento\Customer\Block\Account\SortLinkInterface" name="promotion-wishlist-claimed">
                <arguments>
                    <argument name="path" xsi:type="string">marvelic_promolists/wishlist/index</argument>
                    <argument name="label" xsi:type="string" translate="true">My Favorite Coupon</argument>
                    <argument name="sortOrder" xsi:type="number">218</argument>
                    <argument name="css_class" xsi:type="string">promo_wishlist</argument>
                </arguments>
            </block>
            <block class="Magento\Customer\Block\Account\SortLinkInterface" name="promotion-my-coupon">
                <arguments>
                    <argument name="path" xsi:type="string">marvelic_promolists/mycoupon/index</argument>
                    <argument name="label" xsi:type="string" translate="true">My Coupon</argument>
                    <argument name="sortOrder" xsi:type="number">219</argument>
                    <argument name="css_class" xsi:type="string">my_coupon</argument>
                </arguments>
            </block>
        </referenceBlock>

    </body>
</page>
